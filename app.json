{
  "expo": {
    "name": "QDC On Demand",
    "slug": "on-demand",
    "version": "15.0.3",
    "orientation": "portrait",
    "icon": "./src/assets/default-logo.png",
    "scheme": "QDCDemo",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/splash-screen.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.qdccustomer.cleankart",
      "buildNumber": "16",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "CFBundleLocalizations": [
          "en",
          "en"
        ],
        "LSApplicationQueriesSchemes": [
          "whatsapp",
          "whatsapp"
        ],
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "aps-environment": "development"
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          }
        ]
      },
      "associatedDomains": [
        "applinks:referral.quickdrycleaning.com"
      ],
      "appleTeamId": "2H9MCN975Q"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "splash": {
        "image": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF",
        "resizeMode": "cover"
      },
      "package": "com.quickdrycleaning.customerapp",
      "versionCode": 16,
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCqzYeQs5SU4tmoumyEmBIIY932dRqtjUM"
        }
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": {
            "scheme": "https",
            "host": "referral.quickdrycleaning.com",
            "pathPrefix": "/qdcdemo"
          },
          "category": [
            "DEFAULT",
            "BROWSABLE"
          ]
        },
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": {
            "scheme": "https",
            "host": "play.google.com",
            "pathPrefix": "/store/apps/details"
          },
          "category": [
            "DEFAULT",
            "BROWSABLE"
          ]
        }
      ],
      "runtimeVersion": "qdc-on-demand"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-localization",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "$(PRODUCT_NAME) would like to use your current location to display near by stores. This information is secure and not shared with any third-party.",
          "locationAlwaysPermission": "$(PRODUCT_NAME) would like to use your current location to display near by stores. This information is secure and not shared with any third-party.",
          "locationWhenInUsePermission": "$(PRODUCT_NAME) would like to use your current location to display near by stores. This information is secure and not shared with any third-party."
        }
      ],
      "expo-build-properties",
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          },
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ],
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": false,
          "supportsPictureInPicture": false
        }
      ],
      [
        "expo-notifications",
        {
          "androidMode": "fcm",
          "icon": "./src/assets/notification-icon.png",
          "googleServicesFile": "./google-services.json",
          "enableBackgroundRemoteNotifications": false,
          "ios": {
            "googleServicesFile": "./GoogleService-Info.plist"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "linking": {
        "prefixes": [
          "https://referral.quickdrycleaning.com/QDCDemo/",
          "QDCDemo://"
        ]
      },
      "eas": {
        "projectId": "868cb649-3530-4a6d-973e-f8cc329462a7"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "wesencedev"
  }
}